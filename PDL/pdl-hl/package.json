{
  "name": "pdl-hl",
  "displayName": "pdl-highlighting",
  "description": "Added syntax support for .pdl files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pdl",
      "aliases": ["pdl", "pdl"],
      "extensions": [".pdl"],
      "configuration": "./pdl-config.json"
    }],
    "grammars": [{
      "language": "pdl",
      "scopeName": "source.pdl",
      "path": "./syntaxes/pdl.tmLanguage.json"
    }]
  }
}
